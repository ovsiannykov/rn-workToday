{
  "expo": {
    "name": "Employer",
    "slug": "Employer",
    "privacy": "public",
    "userInterfaceStyle": "automatic",
     "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission":
            "Застосунок отримує доступ до ваших фотографій для коректної роботи."
        }
      ]
    ],
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "resizeMode": "cover",
      "image": "./assets/splash.png"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": false,
      "buildNumber": "1",
      "bundleIdentifier": "com.work.ios",
      "config": {
        "googleMapsApiKey": "AIzaSyBOgtxgzRLUT614o4D7nwepZVGJ2IGgP0Y"
      }
    },
    "android": {
      "package": "com.work",
      "softwareKeyboardLayoutMode": "pan",
      "versionCode": 1,
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyBOgtxgzRLUT614o4D7nwepZVGJ2IGgP0Y"
        }
      }
    },
    "packagerOpts": {
      "config": "metro.config.js",
      "sourceExts": [
        "expo.ts",
        "expo.tsx",
        "expo.js",
        "expo.jsx",
        "ts",
        "tsx",
        "js",
        "jsx",
        "json",
        "wasm",
        "svg"
      ]
    }
  }
}
